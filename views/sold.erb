<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Aling Nena's</title>

    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Lobster" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Cabin" rel="stylesheet" type="text/css">
	<link href="/css/style.css" rel="stylesheet" type="text/css">
  </head>

  <body>
	<div class="banner">
		<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
			<div class="container">
			    <div class="navbar-header">
			      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
		            <span class="sr-only">Toggle navigation</span>
		            <span class="icon-bar"></span>
		            <span class="icon-bar"></span>
		            <span class="icon-bar"></span>
		          </button>
			      <a class="navbar-brand" href="#"><h6>Aling Nena's</h6></a>
			    </div><!--navbar-header-->
			    <div class="collapse navbar-collapse">
		          <ul class="nav navbar-nav">
		            <li><a href="/">Home</a></li>
		            <li><a href="/about">About</a></li>
		            <li><a href="/products">Products</a></li>
		          </ul>
		        </div><!--/.nav-collapse -->
			</div><!--container-->
		</div><!--navbar-->
		<div class="label">
			<img src="/images/label.png" id="label"/>
		</div><!--label-->
	</div><!--banner-->
	
	
<div class="content">
	<div class="col-md-12">
	    <div class="row">
		<div class="col-sm-1"></div>
	    <div class="col-sm-5">
	    <div class="left-col">
			<h1> <%= @status %> </h1>
		</br>
			<p><b> Total Cost: <%= @payment %> </b></p>
			<p><b> Change: <%= @total_change %> </b></p>

	<p> ones: <%= @cash[:ones].to_i %> </br>fives: <%= @cash[:fives].to_i%> </br>tens: <%= @cash[:tens] %> </br>twenties: <%= @cash[:twenties].to_i %> </br>fifties: <%= @cash[:fifties].to_i %> </br>hundreds: <%= @cash[:hundreds].to_i %> </br>five hundreds: <%= @cash[:five_hundreds].to_i %> </br>thousands: <%= @cash[:thousands].to_i %></p>
	
	<a href="/products"><h4>Back to Products <span class="glyphicon glyphicon-chevron-left"/><span class="glyphicon glyphicon-chevron-left"/></h4></a> 
		
	    <!--Sidebar content-->
		</div>
		</div>
		<div class="col-sm-1"></div>
	    <div class="col-sm-4">
	    <div class="right-col">
		<img src="/images/window.png" id="window"/>
	    <!--Body content-->
	    </div>
		</div>
		<div class="col-sm-1"></div>
	    </div>
    </div>
</div><!--content-->

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>

  </body>
</html>
